#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/node_modules:/userroot/website/arjata/service/proxy/clash/control/node_modules:/userroot/website/arjata/service/proxy/clash/node_modules:/userroot/website/arjata/service/proxy/node_modules:/userroot/website/arjata/service/node_modules:/userroot/website/arjata/node_modules:/userroot/website/node_modules:/userroot/node_modules:/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules/@typescript-eslint/eslint-plugin/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules/@typescript-eslint/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/typescript@4.6.3/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/node_modules:/userroot/website/arjata/service/proxy/clash/control/node_modules:/userroot/website/arjata/service/proxy/clash/node_modules:/userroot/website/arjata/service/proxy/node_modules:/userroot/website/arjata/service/node_modules:/userroot/website/arjata/node_modules:/userroot/website/node_modules:/userroot/node_modules:/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules/@typescript-eslint/eslint-plugin/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules/@typescript-eslint/node_modules:/userroot/website/arjata/service/proxy/clash/control/git/clash-dashboard/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.17.0_689ff565753ecf7c3328c07fad067df5/node_modules"
